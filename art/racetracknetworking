from tealight.net import connect, send
connect("racetracktest")
carNumber=0
x=0
users={}
lastsend={}
def handle_message(message):
  global carNumber, users
  if "type" in message:
    type = message['type']
    if type == "heartbeat":
       lastsend[message['user_id']] = {"last-sent": message['payload']}
       print users
    elif type == "registration":
      users[] = message['payload']
      print users
